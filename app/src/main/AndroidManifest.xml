<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.id_dev_fire">

    <application
        android:name=".Home"
        android:allowBackup="true"
        android:icon="@drawable/logonum_3"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logonum_3"
        android:supportsRtl="true"
        android:theme="@style/Theme.IDDevFire">
        <activity android:name=".ui.register.RegisterActivity"></activity>
        <activity android:name=".ui.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.IDDevFire.NoActionBar"></activity>
    </application>

</manifest>